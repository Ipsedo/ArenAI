<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="SetUpAxis">
		<attr name="axis" format="enum">
			<enum name="direction" value="0"/>
			<enum name="speed" value="1"/>
			<enum name="canon" value="2"/>
			<enum name="turret" value="3"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="SetUpButton">
		<attr name="button" format="enum">
			<enum name="brake" value="10"/>
			<enum name="respawn" value="11"/>
			<enum name="fire" value="12"/>
		</attr>
	</declare-styleable>
</resources>